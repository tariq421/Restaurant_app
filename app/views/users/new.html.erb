<h1>Sign up!</h1>

<div class="row">
  <div class="twelve columns">

  <%= form_for @user do |s| %>
    <%= s.label :username %>
    <%= s.text_field :username %>

    <%= s.label :password %>
    <%= s.password_field :password %>

    <%= s.submit "Add me!" %>
  </div>
</div>

<a href="/">Log in</a>

<% end %>
